    "main": "index.js",
    "private": true,
    "scripts": {
        "build": "next build",
        "build:cf": "@cloudflare/next-on-pages",
        "cf:dev": "wrangler pages dev",
        "db:generate": "prisma generate",
        "db:migrate": "wrangler d1 migrations apply",
        "db:seed": "wrangler d1 execute --file=./prisma/seed.sql",
        "deploy": "wrangler pages deploy",
        "dev": "next dev --turbopack",
        "format": "run-s format:*",
        "format:eslint": "eslint --fix .",
        "format:fixpack": "fixpack",
        "format:prettier": "prettier --write .",
        "lint": "run-p lint:*",
        "lint:actionlint": "bash -c 'if which actionlint >/dev/null 2>&1 && actionlint --version >/dev/null 2>&1; then actionlint .github/workflows/*.yml; elif command -v docker >/dev/null 2>&1; then echo \"Docker経由でactionlintを実行します...\"; docker run --rm -v $(pwd):/repo --workdir /repo rhysd/actionlint:latest -color; else echo \"actionlintまたはDockerが見つかりません。インストールしてください\"; exit 1; fi'",
        "lint:cspell": "cspell lint .",
        "lint:eslint": "eslint .",
        "lint:prettier": "prettier --check .",
        "lint:secretlint": "secretlint .",
        "safe-amend": "./scripts/safe-amend.sh",
        "start": "next start",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit",
        "validate": "run-s type-check lint test build"
    }
}
