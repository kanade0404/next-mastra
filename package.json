{
    "name": "next-mastra",
    "version": "0.1.0",
    "private": true,
    "description": "Next.js + Mastra Chat LLM Template with Cloudflare integration",
    "main": "index.js",
    "scripts": {
        "build": "next build",
        "build:cf": "@cloudflare/next-on-pages",
        "cf:dev": "wrangler pages dev",
        "db:generate": "prisma generate",
        "db:migrate": "wrangler d1 migrations apply",
        "db:seed": "wrangler d1 execute --file=./prisma/seed.sql",
        "deploy": "wrangler pages deploy",
        "dev": "next dev --turbopack",
        "format": "run-s format:*",
        "format:package": "sort-package-json package.json && dprint fmt package.json",
        "format:oxlint": "oxlint --fix .",
        "format:dprint": "dprint fmt",
        "lint": "run-p lint:*",
        "lint:cspell": "cspell lint .",
        "lint:oxlint": "oxlint .",
        "lint:dprint": "dprint check",
        "lint:secretlint": "secretlint .",
        "safe-amend": "./scripts/safe-amend.sh",
        "start": "next start",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit",
        "validate": "run-s type-check lint test build"
    },
    "dependencies": {
        "@clerk/nextjs": "^6.22.0",
        "@cloudflare/d1": "^1.4.1",
        "@cloudflare/workers-types": "^4.20250614.0",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/sdk-node": "^0.202.0",
        "@pinecone-database/pinecone": "^4.0.1",
        "@prisma/client": "^6.9.0",
        "@sentry/nextjs": "^9.29.0",
        "@tailwindcss/postcss": "^4.1.10",
        "@tanstack/react-query": "^5.80.7",
        "autoprefixer": "^10.4.21",
        "brevo": "^1.0.0",
        "clsx": "^2.1.1",
        "csurf": "^1.11.0",
        "helmet": "^8.1.0",
        "jose": "^6.0.11",
        "ladle": "^0.0.0",
        "mastra": "^0.10.5",
        "neverthrow": "^8.2.0",
        "next": "15.3.3",
        "openai": "^4.82.1",
        "pdf-parse": "^1.1.1",
        "pino": "^9.7.0",
        "postcss": "^8.5.5",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "superjson": "^2.2.2",
        "svix": "^1.67.0",
        "tailwindcss": "^4.1.10",
        "zod": "^3.25.64",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "^1.13.7",
        "@commitlint/config-conventional": "^19.8.1",
        "@eslint/mcp": "^0.1.0",
        "@modelcontextprotocol/sdk": "^1.13.0",
        "@secretlint/secretlint-rule-no-dotenv": "^10.1.0",
        "@secretlint/secretlint-rule-pattern": "^10.1.0",
        "@secretlint/secretlint-rule-preset-recommend": "^10.1.0",
        "@secretlint/secretlint-rule-secp256k1-privatekey": "^10.1.0",
        "@secretlint/secretlint-rule-slack": "^10.1.0",
        "@types/node": "^24.0.1",
        "@types/pdf-parse": "^1.1.4",
        "@types/react": "^19.1.8",
        "commitlint": "^19.8.1",
        "cross-env": "^7.0.3",
        "cspell": "^9.1.1",
        "depcheck": "^1.4.7",
        "dotenv": "^16.5.0",
        "dprint": "^0.50.0",
        "esbuild": "^0.25.5",
        "lefthook": "^1.11.13",
        "msw": "^2.10.2",
        "npm-check-updates": "^18.0.1",
        "npm-run-all": "^4.1.5",
        "oxlint": "^1.2.0",
        "prisma": "^6.9.0",
        "react-docgen": "^8.0.0",
        "renovate": "^40.55.0",
        "secretlint": "^10.1.0",
        "sort-package-json": "^3.2.1",
        "testcontainers": "^11.0.3",
        "tsx": "^4.20.3",
        "typedoc": "^0.28.5",
        "typescript": "^5.8.3",
        "vitest": "^3.2.3",
        "wrangler": "^4.20.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    }
}
